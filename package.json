{
  "name": "dicedb-driver",
  "version": "1.2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build": "rm -rf dist && npx tsup index.ts --dts --format esm,cjs --target node18 --external all",
    "compile": "protoc -I=./dicedb-protos --es_out=target=ts:./proto dicedb-protos/*.proto",
    "codegen": "node --loader ts-node/esm scripts/codegen.ts",
    "test": "mocha tests/index.test.js"
  },
  "keywords": [
    "Database",
    "DiceDB",
    "Database driver",
    "Redis",
    "Key-value store",
    "In-memory database",
    "js-driver",
    "client",
    "kv-store"
  ],
  "author": "Aditya DS",
  "license": "MIT",
  "description": "DiceDB Driver is a lightweight, promise-based database driver for DiceDB with built-in connection pooling.",
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.17",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "chai": "^5.2.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aditya-ds-1806/dicedb-js.git"
  },
  "homepage": "https://aditya-ds-1806.github.io/dicedb-driver",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
