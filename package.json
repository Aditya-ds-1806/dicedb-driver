{
  "name": "dice-js",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build": "rm -rf build && npx tsup index.ts --dts --format esm,cjs --target node18 --external all",
    "compile": "protoc -I=./dicedb-protos --es_out=target=ts:./generated dicedb-protos/cmd.proto",
    "dev": "DEBUG=DiceDB* nodemon --ext ts --exec ts-node index.ts",
    "start": "nodemon -r ts-node/register index.js",
    "codegen": "node --loader ts-node/esm scripts/codegen.ts"
  },
  "keywords": [],
  "author": "Aditya DS",
  "license": "MIT",
  "description": "DiceJS is an easy to use, promise-based database client for DiceDB, that supports connection pooling for optimal performance.",
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.17",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aditya-ds-1806/dicedb-js.git"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
